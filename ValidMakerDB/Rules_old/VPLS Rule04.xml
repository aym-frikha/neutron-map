<?xml version="1.0" standalone="yes"?>
<Rule xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file:/C:/Program%20Files/validmaker/Data/Schema/Rule.xsd" name="VPLS Rule04">
  <Description>dans deux arbre differents si "l2 vfi" sont egaux alors la valeur de "vpn id" seront egaux</Description>
  <Conditions>
    <Condition>
      <LogicalOp>NONE</LogicalOp>
      <Negated>false</Negated>
      <Operation>NodeValueEqual</Operation>
      <LeftTree>1</LeftTree>
      <LeftOperand>
        <Node>
          <ServiceRef>VPLS</ServiceRef>
          <Name>l2 vfi</Name>
          <GUID>1</GUID>
        </Node>
      </LeftOperand>
      <RightTree>2</RightTree>
      <RightOperand>
        <Node>
          <ServiceRef>VPLS</ServiceRef>
          <Name>l2 vfi</Name>
          <GUID>1</GUID>
        </Node>
      </RightOperand>
    </Condition>
  </Conditions>
  <Actions>
    <Action>
      <LogicalOp>NONE</LogicalOp>
      <Negated>false</Negated>
      <Operation>NodeValueEqual</Operation>
      <LeftTree>1</LeftTree>
      <LeftOperand>
        <Node>
          <ServiceRef>VPLS</ServiceRef>
          <Name>vpn id</Name>
          <GUID>2</GUID>
        </Node>
      </LeftOperand>
      <RightTree>2</RightTree>
      <RightOperand>
        <Node>
          <ServiceRef>VPLS</ServiceRef>
          <Name>vpn id</Name>
          <GUID>2</GUID>
        </Node>
      </RightOperand>
    </Action>
  </Actions>
</Rule>
